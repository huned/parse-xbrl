{
  "name": "parse-xbrl",
  "description": "Module to parse xbrl documents and output json.",
  "version": "1.1.4",
  "type": "module",
  "main": "dist/index.js",
  "typings": "index.d.ts",
  "config": {
    "build_directory": "dist"
  },
  "scripts": {
    "prepare": "babel src -d $npm_package_config_build_directory -s && echo '{\"type\":\"commonjs\"}' > $npm_package_config_build_directory/package.json",
    "prebuild": "rimraf $npm_package_config_build_directory",
    "build": "npm run prepare",
    "pretest": "npm run build",
    "test": "mocha --spec 'test/**/*.spec.js' ",
    "test:watch": "npm run test -- --watch",
    "test:learning": "mocha test/fact.spec.learning.js",
    "pretty": "prettier --write './{src,test}/**/*.js'"
  },
  "files": [
    "index.d.ts",
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "author": "Emily Coco <emilymcoco@gmail.com>",
  "contributors": [
    {
      "name": "Alejandro Encalado",
      "url": "https://github.com/FaradayDetu"
    },
    {
      "name": "Ricard Mallafre",
      "email": "r.mallafre.lopez@gmail.com",
      "url": "https://github.com/nikensss"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "core-js": "^3.10.0",
    "lodash": "^4.17.21",
    "xml2json": "^0.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-standard": "^1.3.3",
    "mocha": "^8.3.2",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:emilycoco/parse-xbrl.git"
  },
  "keywords": [
    "xbrl",
    "xml",
    "parse",
    "financial",
    "edgar",
    "sec"
  ],
  "bugs": {
    "url": "git@github.com:emilycoco/parse-xbrl.git/issues"
  }
}
